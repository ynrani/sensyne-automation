{
  "name": "sensyne-automation",
  "version": "0.0.1",
  "description": "Sensyne Health Automated BDD framework that uses webdriver.io, JavaScript and cucumber-js",
  "repository": {
    "type": "git",
    "url": ""
  },
  "main": "test/config/env/dev/dev.conf.js",
  "scripts": {
    "postinstall": "selenium-standalone install",
    "dev": "wdio test/config/env/dev/dev.conf.js",
    "devbs": "wdio test/config/env/dev/devbs.conf.js",
    "selenium": "npm run removeReports && selenium-standalone start",
    "removeReports": "node ./test/config/bin/removeReports.js",
    "report": "allure serve test/reports/allure-results"
  },
  "keywords": [
    "test",
    "automation",
    "selenium",
    "webdriver",
    "webdriverIO"
  ],
  "author": "Charlie Kurban",
  "license": "Sensyne Health",
