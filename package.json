    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.3.13",
    "browserstack-local": "^1.3.4",
    "chai": "3.5.0",
    "chai-as-promised": "5.3.0",
    "chromedriver": "^2.45.0",
    "commander": "^2.15.1",
    "cucumber": "^0.10.4",
    "deepmerge": "^2.1.1",
    "fibers": "^2.0.2",
    "googleapis": "^30.0.0",
    "i18n": "^0.8.3",
    "interval-promise": "^1.2.0",
    "jsonpath": "^1.0.0",
